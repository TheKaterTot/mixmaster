<%= form_for ([@user, @playlist]) do |f| %>
<%= render partial: "shared/errors", locals: { target: @playlist } %>
<%= f.collection_check_boxes :song_ids, @songs, :id, :title do |song| %>
  <div>
  <%= song.label { song.check_box + song.text } %>
  </div>
<% end %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.submit %>
<% end %>
